{
  "name": "badgr-ui",
  "version": "2.16.1",
  "description": "Badgr UI",
  "scripts": {
    "test": "npm run test:ci",
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack",
    "sass": "node-sass",
    "live-server": "live-server",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "styleguide": "styleguide",
    "protractor": "protractor",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "clean:dist": "npm run rimraf -- dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "node $NODE_DEBUG_OPTION ./node_modules/webpack/bin/webpack.js --config config/webpack.dev.js --progress --profile --colors",
    "build:styling": "npm run build:dev; mkdir dist/styling; cat ./src/breakdown/static/css/reset.css > dist/styling/badgr-ui.css; cat dist/theme-default.*.css >> dist/styling/badgr-ui.css; cp -r dist/assets src/breakdown/static/images dist/styling",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "node $NODE_DEBUG_OPTION ./node_modules/webpack/bin/webpack.js  --config config/webpack.prod.js --profile --colors && npm run write-version && npm run css:prod && npm run build:widgets",
    "build:widgets": "node $NODE_DEBUG_OPTION ./node_modules/webpack/bin/webpack.js --config config/webpack.widgets.js --colors --bail",
    "write-version": "node get-git-version --txt > dist/version.txt && node get-git-version > dist/version.json",
    "server": "npm run server:dev",
    "server:dev": "node $NODE_DEBUG_OPTION node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack.dev.js --progress --profile --colors --watch --host 0.0.0.0 --content-base src/",
    "server:prod": "live-server --entry-file=index.local.html --open=/login dist",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test:ci": "karma start config/karma-ci.conf.js",
    "test:debug": "karma start config/karma-debug.conf.js",
    "ci": "npm run lint && npm test && npm run e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "css:build": "npm run css:clean && npm run css:build:sass && npm run css:build:assets && npm run css:copy:js",
    "css:build:assets": "npm run css:imagemin && npm run css:fontmin",
    "css:build:sass": "npm run css:node-sass && npm run css:postcss",
    "css:clean": "npm run rimraf -- dist/styleguide && npm run rimraf -- temp",
    "css:copy:js": "cp -r src/breakdown/static/js temp",
    "css:dev": "npm run css:build && fractal start --sync",
    "css:prod": "npm run css:build && fractal build",
    "css:fontmin": "fontmin src/breakdown/static/scss/fonts/* temp/fonts",
    "css:imagemin": "imagemin src/breakdown/static/scss/images/* -o temp/images",
    "css:node-sass": "node-sass -o temp/ src/breakdown/static/scss/theme-default.scss",
    "css:postcss": "postcss -u autoprefixer -r temp/theme-default.css",
    "css:watch": "onchange 'src/breakdown/static/scss/**/*.scss' -- npm run css:build:sass",
    "validana-compatibility": "node $NODE_DEBUG_OPTION ./validana-compatibility.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^4.1.2",
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.1.2",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.2",
    "@angular/http": "^4.1.2",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.1.2",
    "@angular/router": "^4.1.2",
    "@angular/upgrade": "^4.1.2",
    "@angularclass/request-idle-callback": "1.0.7",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@types/fontfaceobserver": "0.0.5",
    "@types/karma-jasmine": "0.0.29",
    "@types/marked": "0.0.28",
    "@types/validator": "^6.3.0",
    "angular2-autosize": "^1.0.1",
    "angulartics2": "2.0.0",
    "base64-image-utils": "0.2.0",
    "clipboard": "1.6.1",
    "core-js": "^2.4.1",
    "css-element-queries": "^0.4.0",
    "detect-browser": "2.0.0",
    "dialog-polyfill": "0.4.7",
    "file-saver": "^1.3.8",
    "font-awesome": "4.7.0",
    "fontfaceobserver": "^2.0.9",
    "handlebars-helpers": "^0.8.2",
    "ie-shim": "^0.1.0",
    "intl": "1.2.5",
    "jasmine-promises": "0.4.1",
    "markdown-to-html-pipe": "^1.2.0",
    "ngc": "^1.0.0",
    "postcss-cssnext": "2.10.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "^5.3.0",
    "systemjs": "0.20.12",
    "tether-tooltip": "^1.2.0",
    "tiny-sha256": "1.0.2",
    "tslib": "^1.7.0",
    "validana-client": "git+https://github.com/Coinversable/validana-client.git",
    "validator": "^9.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@frctl/fractal": "^1.1.7",
    "@ngtools/webpack": "^1.3.1",
    "@types/core-js": "0.9.41",
    "@types/fabric": "1.5.23",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.12",
    "@types/selenium-webdriver": "3.0.1",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.14",
    "angular-router-loader": "0.6.0",
